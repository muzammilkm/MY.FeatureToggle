<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>MY.FeatureToggle.Providers.LaunchDarkly</AssemblyName>
    <Authors>Muzammil Khaja Mohammed</Authors>
    <Description>Provide better management of feature flags of Launch Darkly</Description>
    <PackageProjectUrl>https://github.com/muzammilkm/MY.FeatureToggle</PackageProjectUrl>
    <RepositoryUrl>https://github.com/muzammilkm/MY.FeatureToggle</RepositoryUrl>
    <PackageTags>Feature Toggle; Flag; Launch Darkly; LaunchDarkly; dotnet; core; inject; featurefalg; featuretoggle;</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>3.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LaunchDarkly.ServerSdk" Version="8.2.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="8.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\MY.FeatureToggle\MY.FeatureToggle.csproj" />
  </ItemGroup>

</Project>
